(set-logic LIA)

(define-var x1 Int (Constant Int))
(define-var x2 Int (Constant Int))
(define-var o Int (Constant Int))

(relations ((= o (max2 x1 x2))))

(generator ((AP Bool ((= I I) (<= I I) (>= I I) (> I I) (< I I) (distinct I I)))
            (I Int (x1 x2 o))))

(example ((Int ((Constant Int)))))

(define-fun max2 ((x1 Int) (x2 Int)) Int (ite (<= x2 x1) x1 x2))
